#include <bits/stdc++.h>
#include <climits>
using namespace std;

#ifdef LOCAL
#include "algo/debug.h"
#else
#define debug(...) 42
#endif
#define mem(a,b) memset(a,b,sizeof(a))
#define debug_arr(a, n) \
    cerr << "[" << #a << "] = {"; \
    for (int i = 0; i < (n); i++) cerr << (i ? "," : "") << a[i]; \
    cerr << "}\n";


#define all(x) (x).begin(), (x).end()
#define ll long long
#define MAXN 1000005;
int a[1000005];
int dp1[2005][2005];
void solve() {
    int n, m;
    while ( cin >> m && cin >> n) {
        // swap(n,m);
        for (int i = 0; i < n; i++) {
            cin >> a[i];
            dp1[i][1] = a[i];
        }
        debug(n , m);
        cerr << dp1[0][1] << " ";
        for (int i = 1; i < n; i++) {
            dp1[i][1] = max(dp1[i][1], dp1[i - 1][1] + a[i]);
            cerr << dp1[i][1] << " ";
        }
        cerr << "\n";
        for (int i = 2; i <= m; i++) {
            for (int j = i - 1; j < n; j++) {
                if (j == i - 1) {
                    dp1[j][i] = a[j] + dp1[j - 1][i - 1];
                } else {
                     int temp = -0x3f3f3f3f;
                     for (int k = j - 1; k >= i - 2; k--) {
                        temp = max(temp, dp1[k][i - 1]);
                     }
                     dp1[j][i] = max(dp1[j - 1][i], temp + a[j]);
                }
                cerr << dp1[j][i] << " ";
            }
            cerr << "\n";
        }
        int ans = -0x3f3f3f3f;
        for (int i = m - 1; i < n; i++) {
            ans = max(ans, dp1[i][m]);
        }
        cout << ans << "\n";
        break;
    }

}
int dp[1000005];
int pre[1000005];
void solve1() {
    int n, m;
    cin >> m >> n;
    for (int i = 1; i <= n; i++) {
        cin >> a[i];
    }
    for (int i = 1; i <= m; i++) {
        dp[i] = max(dp[i], dp[i - 1] + a[i]);
    }
    int temp = 0;
    for (int i = 1; i <= m; i++) {
        temp = -1e9;
        for (int j = i; j <= n; j++) {
            dp[j] = max(dp[j - 1], pre[j - 1]) + a[j];
            pre[j - 1] = temp;
            temp = max(dp[j], temp);
        }
    }
    cout << temp << "\n";
}
//sk-a0337987fa3844b4bdc28ba028f991f6
int main() {
    // srand(time(0));
    ios::sync_with_stdio(0);
    cout.tie(0), cin.tie(0);
    int T = 1;
    // cin >> T;
    while(T--) solve1();
    return 0;
}